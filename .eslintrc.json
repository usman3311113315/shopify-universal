{
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "es2021": true
  },
  "rules": {
    "prettier/prettier": ["error", {
      "singleQuote": true,
      "printWidth": 100,
      "tabWidth": 2,
      "trailingComma": "es5",
      "bracketSameLine": false
    }]
  }
}
